<?php

// Brute force way of looping to 99999 and checking whether the value is same forward and backward
$palins = [];
for ($i = 1; $i < 100000; $i++) {
   for ($y = 1; $y < 100000; $y++) {
       $value = $i * $y;
       $value = (string) $value;
       if ( $value ===  strrev($value) && !in_array($value, $palins)) {
          $palins[] = $value;
          var_dump($value . "---" . $i . "----" . $y);
       }
   }
}

?>
